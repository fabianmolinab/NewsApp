{
  "name": "news-app",
  "version": "1.0",
  "private": true,
  "dependencies": {
    "react": "17",
    "react-dom": "17",
    "react-router-dom": "6.2.2",
    "react-scripts": "5.0.0",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "eslintConfig": {
